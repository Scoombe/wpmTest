{
  "name": "wpmtest",
  "version": "1.3.5",
  "description": "A simple class for a words per minute test.",
  "directories": {
    "test": "test"
  },
  "main": "bin/wordsPerMinTest.js",
  "scripts": {
    "test": "mocha -r ts-node/register test/*.test.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json 'src/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/*.ts' --fix"
  },
  "pre-push": [
    "test",
    "build"
  ],
  "pre-commit": [
    "build",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "http://www.github.com/scoombe/wpmTest"
  },
  "keywords": [
    "wpm",
    "word",
    "per",
    "minute",
    "test"
  ],
  "author": "scoombe",
  "license": "ISC",
  "dependencies": {
    "random-words": "^1.1.0",
    "timer-stopwatch": "git+https://github.com/MickCrozier/timer-stopwatch.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.2",
    "chai": "^4.1.2",
    "git-pre-push": "0.0.5",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
